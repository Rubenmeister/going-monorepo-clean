{
    "name":  "anfitriones-service",
    "$schema":  "../node_modules/nx/schemas/project-schema.json",
    "sourceRoot":  "anfitriones-service/src",
    "projectType":  "application",
    "tags":  [

             ],
    "targets":  {
                    "build":  {
                                  "outputs":  [
                                                  "{options.outputPath}"
                                              ],
                                  "defaultConfiguration":  "production",
                                  "executor":  "@nx/webpack:webpack",
                                  "configurations":  {
                                                         "development":  {
                                                                             "mode":  "development"
                                                                         },
                                                         "production":  {
                                                                            "inspect":  false,
                                                                            "optimization":  true,
                                                                            "mode":  "production",
                                                                            "extractLicenses":  true
                                                                        }
                                                     },
                                  "options":  {
                                                  "assets":  [
                                                                 "anfitriones-service/src/assets"
                                                             ],
                                                  "isolatedConfig":  true,
                                                  "tsConfig":  "anfitriones-service/tsconfig.app.json",
                                                  "outputPath":  "dist/anfitriones-service",
                                                  "generatePackageJson":  true,
                                                  "target":  "node",
                                                  "webpackConfig":  "anfitriones-service/webpack.config.js",
                                                  "main":  "anfitriones-service/src/main.ts",
                                                  "compiler":  "tsc"
                                              }
                              },
                    "prune-lockfile":  {
                                           "dependsOn":  [
                                                             "build"
                                                         ],
                                           "cache":  true,
                                           "executor":  "@nx/js:prune-lockfile",
                                           "outputs":  [
                                                           "{workspaceRoot}/dist/anfitriones-service/package.json",
                                                           "{workspaceRoot}/dist/anfitriones-service/package-lock.json"
                                                       ],
                                           "options":  {
                                                           "buildTarget":  "build"
                                                       }
                                       },
                    "copy-workspace-modules":  {
                                                   "dependsOn":  [
                                                                     "build"
                                                                 ],
                                                   "cache":  true,
                                                   "outputs":  [
                                                                   "{workspaceRoot}/dist/anfitriones-service/workspace_modules"
                                                               ],
                                                   "executor":  "@nx/js:copy-workspace-modules",
                                                   "options":  {
                                                                   "buildTarget":  "build"
                                                               }
                                               },
                    "prune":  {
                                  "dependsOn":  [
                                                    "prune-lockfile",
                                                    "copy-workspace-modules"
                                                ],
                                  "executor":  "nx:noop"
                              },
                    "serve":  {
                                  "continuous":  true,
                                  "executor":  "@nx/js:node",
                                  "defaultConfiguration":  "development",
                                  "dependsOn":  [
                                                    "build"
                                                ],
                                  "options":  {
                                                  "buildTarget":  "anfitriones-service:build",
                                                  "runBuildTargetDependencies":  false
                                              },
                                  "configurations":  {
                                                         "development":  {
                                                                             "buildTarget":  "anfitriones-service:build:development"
                                                                         },
                                                         "production":  {
                                                                            "buildTarget":  "anfitriones-service:build:production"
                                                                        }
                                                     }
                              },
                    "test":  {
                                 "options":  {
                                                 "passWithNoTests":  true
                                             }
                             }
                }
}
