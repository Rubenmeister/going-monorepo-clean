{
  "name": "user-auth-service",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "user-auth-service/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/user-auth-service",
        "main": "user-auth-service/src/main.ts",
        "tsConfig": "user-auth-service/tsconfig.app.json",
        "assets": ["user-auth-service/src/assets"],
        "generatePackageJson": true,
        "isolatedConfig": true,
        "webpackConfig": "user-auth-service/webpack.config.js"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "user-auth-service:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "user-auth-service:build:development"
        },
        "production": {
          "buildTarget": "user-auth-service:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/user-auth-service"],
      "options": {
        "jestConfig": "user-auth-service/jest.config.cts",
        "passWithNoTests": true
      }
    }
  }
}
